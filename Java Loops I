import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        int i,c;
        Scanner in=new Scanner(System.in);
        int n=in.nextInt();
        for(i=1;i<=10;i++){
            c=i*n;
            System.out.println(n+" "+"x"+" "+i+" "+"="+" "+c);
        }
    }
}
